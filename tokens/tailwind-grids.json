{
  "grid": {
    ".ts-grid-col-1": {
      "grid-template-columns": "repeat(1, minmax(0, 1fr));",
      "display": "grid",
      "gap": "var(--grid-gap)",
      "@media ( min-width: 768px )": {
        "columns": "var(--grid-columns-md);",
        "gap": "var(--grid-gap-md)"
      },
      "@media ( min-width: 1026px )": {
        "columns": "var(--grid-columns-lg);",
        "gap": "var(--grid-gap-lg)"
      },
      "@media ( min-width: 1440px )": {
        "columns": "var(--grid-columns-xl);"
      }
    },
