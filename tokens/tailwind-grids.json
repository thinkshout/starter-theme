{
  "grid": {
    ".ts-grid-col-2": {
      "display": "grid",
      "grid-template-columns": "repeat(2, minmax(0, 1fr));",
      "gap": "var(--spacer-md)",
      "@media ( min-width: 768px )": {
        "gap": "var(--spacer-xl)"
      },
      "@media ( min-width: 1025px )": {
        "gap": "36px"
      }
    },
    ".ts-grid-col-3": {
      "display": "grid",
      "grid-template-columns": "repeat(3, minmax(0, 1fr));",
      "gap": "var(--spacer-md)",
      "@media ( min-width: 768px )": {
        "gap": "var(--spacer-xl)"
      },
      "@media ( min-width: 1025px )": {
        "gap": "36px"
      }
    },
    ".ts-grid-col-4": {
      "display": "grid",
      "grid-template-columns": "repeat(4, minmax(0, 1fr));",
      "gap": "var(--spacer-md)",
      "@media ( min-width: 768px )": {
        "gap": "var(--spacer-xl)"
      },
      "@media ( min-width: 1025px )": {
        "gap": "36px"
      }
    },
    ".ts-grid-col-6": {
      "display": "grid",
      "grid-template-columns": "repeat(6, minmax(0, 1fr));",
      "gap": "var(--spacer-md)",
      "@media ( min-width: 768px )": {
        "gap": "var(--spacer-xl)"
      },
      "@media ( min-width: 1025px )": {
        "gap": "36px"
      }
    },
    ".ts-grid-7-5": {
      "display": "grid;",
      "columns": "12;",
      "gap": "var(--spacer-md);",
      "div:nth-of-type(1)": {
        "grid-column": "1 / span 7;"
      },
      "div:nth-of-type(2)": {
        "grid-column": "8 / span 5;"
      },
      "@media ( min-width: 768px )": {
        "gap": "var(--spacer-xl)"
      },
      "@media ( min-width: 1025px )": {
        "gap": "36px"
      }
    },
    ".ts-grid-9-3": {
      "display": "grid;",
      "columns": "12;",
      "gap": "var(--spacer-md);",
      "div:nth-of-type(1)": {
        "grid-column": "1 / span 9;"
      },
      "div:nth-of-type(2)": {
        "grid-column": "10 / span 3;"
      },
      "@media ( min-width: 768px )": {
        "gap": "var(--spacer-xl)"
      },
      "@media ( min-width: 1025px )": {
        "gap": "36px"
      }
    },
    ".ts-grid-center": {
      "margin-left": "auto;",
      "margin-right": "auto;",
      "@media ( min-width: 768px )": {
        "max-width": "600px"
      }
    }
  }
}
